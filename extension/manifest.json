{
  "manifest_version": 3,
  "name": "Postbank",
  "version": "1.0",
  "description": "Sehe ob eine Investition sinnvoll ist.",
  "host_permissions": [
    "https://alessio.ddnss.de/*",
    "https://amazon.de/*"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}
